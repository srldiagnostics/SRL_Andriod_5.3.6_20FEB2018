public class ProductHeaderListItemData
{
	private String header;

	private String desc;

	public String getHeader()
	{
		return header;
	}

	public void setHeader(final String header)
	{
		this.header = header;
	}

	public String getDesc()
	{
		return desc;
	}

	public void setDesc(final String desc)
	{
		this.desc = desc;
	}

	@Override
	public String toString()
	{
		return "ProductHeaderListItemData{" + "header='" + header + '\'' + ", desc='" + desc + '\'' + '}';
	}
}
